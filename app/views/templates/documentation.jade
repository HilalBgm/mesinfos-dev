mixin displaySubset(s)
  li
    h4= s.Nom
     |&nbsp;(origine :&nbsp; 
     =s.Détenteur
     | ) 

    p= s.Description

    ul.caracteristics
      li
        b Fréquence : 
        =s.Fréquence

      li
        b Latence :
        =s.Latence



h4 DocType :&nbsp;
  =docType.Nom

p= docType.Description

| Sous-ensembles du même type :
ul
  each subset in subsets
    +displaySubset(subset)
